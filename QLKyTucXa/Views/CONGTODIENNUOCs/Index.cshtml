@model IEnumerable<QLKyTucXa.Models.ViewModel_HD>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

@section header{
    Danh Sách Công tơ điện nước
}

<div class="container-fluid">
    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <div class="widget-title">
                    <span class="icon"><i class="icon-th"></i></span>
                    <h5>Danh Sách Công tơ điện nước</h5>
                </div>
                <div class="widget-content nopadding">
                    <table class="table table-bordered data-table">
                        <thead>
                            <tr>
                                <th>Mã phòng</th>
                                <th>Mã Dãy phòng</th>
                                <th>Điện tháng trước</th>
                                <th>Điện tháng này</th>
                                <th>Chỉ số điện</th>
                                <th>Nước tháng trước</th>
                                <th>Nước tháng này</th>
                                <th>Chỉ số nước</th>
                                <th>Tháng điện</th>
                                <th>Năm điện</th>
                                <th>Tháng nước</th>
                                <th>Năm nước</th>
                                <th>Trạng thái điện</th>
                                <th>Trạng thái nước</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr class="gradeX">
                                    <td>@item.MAPHONG</td>
                                    <td>@item.MADAYPHONG</td>
                                    <td>@item.DIENCHISODAU</td>
                                    <td>@item.DIENCHISOCUOI</td>
                                    <td>@item.CHISODIEN</td>
                                    <td>@item.NUOCCHISODAU</td>
                                    <td>@item.NUOCCHISOCUOI</td>
                                    <td>@item.CHISONUOC</td>
                                    <td>@item.THANGDIEN</td>
                                    <td>@item.NAMDIEN</td>
                                    <td>@item.THANGNUOC</td>
                                    <td>@item.NAMNUOC</td>
                                    <td>@item.TRANGTHAIDIEN</td>
                                    <td>@item.TRANGTHAINUOC</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>